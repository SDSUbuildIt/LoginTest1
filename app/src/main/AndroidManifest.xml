<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="buildit.joeycasabar.logintest1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginScreen" />
        <activity
            android:name=".ActionListActivity"
            android:label="@string/title_action_list"
            android:parentActivityName=".LoginScreen"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="buildit.joeycasabar.logintest1.LoginScreen" />
        </activity>
        <activity
            android:name=".ActionDetailActivity"
            android:label="@string/title_action_detail"
            android:parentActivityName=".ActionListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="buildit.joeycasabar.logintest1.ActionListActivity" />
        </activity>
        <activity
            android:name=".Landing1"
            android:label="@string/title_activity_landing1"
            android:parentActivityName=".LoginScreen"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="buildit.joeycasabar.logintest1.LoginScreen" />
        </activity>
    </application>

</manifest>